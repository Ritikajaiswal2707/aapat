{
  "version": 2,
  "name": "aapat-emergency-platform",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/"
    }
  ]
}
